package application;

import javafx.fxml.FXML;

public class SettingsController extends MainWindowController {

	private Main main;
	
	public void setMain(Main main){
		this.main = main;
	}
	
	@FXML
	public void handleNewUser() {
		main.settingsWindow();
	}
	
	@FXML
	public void handleChangePassword() {
		main.settingsWindow();
	}
	
	@FXML
	public void handleUpdateInfo() {
		main.settingsWindow();
	}
	
}
